{% extends 'base.html.twig' %}

{% block title %}Competence{% endblock %}

{% block body %}
    <h1>Competence</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ competence.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ competence.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ competence.description }}</td>
            </tr>
            <tr>
                <th>Date_de_debut</th>
                <td>{{ competence.dateDeDebut ? competence.dateDeDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_de_fin</th>
                <td>{{ competence.dateDeFin ? competence.dateDeFin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Note</th>
                <td>{{ competence.note }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('competence_index') }}">back to list</a>

    <a href="{{ path('competence_edit', {'id': competence.id}) }}">edit</a>

    {{ include('competence/_delete_form.html.twig') }}
{% endblock %}
