<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Home{% endblock %}</title>

        {#    webencore css    #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        {#  webencore fin css #}


        {% block googlefonts %}
            <link href="https://fonts.googleapis.com/css2?family=Philosopher:ital@1&display=swap" rel="stylesheet">
            <link href="https://fonts.googleapis.com/css2?family=Lobster&display=swap" rel="stylesheet">
        {% endblock %}
       {# fin googlefont #}

     {# fontawesome  #}
        {% block fontawesome %}
            <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.13.0/css/all.css" integrity="sha384-Bfad6CLCknfcloXFOyFnlgtENryhrpZCe29RTifKEixXQZ38WheV+i/6YWSzkz3V" crossorigin="anonymous">
        {% endblock %}
    {# fin fontawsome #}
    </head>
    <body class="bg-warning">
    {# mon Nav#}
    <nav class="navbar navbar-expand-lg bg-warning shadow fixed-top">

        {#   icon     #}
        <a class="navbar-brand text-dark font-weight-bolder" href="#"><i class="fab fa-codepen ml-5" style="font-size: 42px"></i></a>
        {#fin de mon icon #}

        <button class="navbar-toggler spinner-border border-dark shadow" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon "></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ml-auto text-center" style="font-size: 20px">
                <li class="nav-item ">
                    <a class="nav-link active font-weight-bold text-dark pr-5" href="{{ path('default') }}">Acceuil</a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link font-weight-bold text-dark pr-5" href="{{ path('apropos') }}">Apropos</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link font-weight-bold text-dark pr-5" href="{{ path('realisation_list') }}">Mes Realisation</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link font-weight-bold text-dark pr-5" href="{{ path('education_list') }}">Mes Parcours</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link font-weight-bold text-dark pr-5" href="{{ path('competence_list') }}">Mes Competences</a>
                </li>

{#                <li class="nav-item">#}
{#                    <a class="nav-link font-weight-bold text-dark pr-5" href="{{path('app_login')}}"></a>#}
{#                </li>#}
            </ul>
        </div>
    </nav>
    {# fin de NAV #}

         <div class="container-fluid">
             <div class="row">
                 <div class="col">
                     {% block body %}{% endblock %}
                 </div>
             </div>
         </div>



        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}


    </body>

</html>
